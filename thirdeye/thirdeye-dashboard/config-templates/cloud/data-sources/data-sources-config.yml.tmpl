
# Please put the mock data source as the first in this configuration.
dataSourceConfigs:
  - className: org.apache.pinot.thirdeye.datasource.sql.SqlThirdEyeDataSource
    properties:
      MySQL:
        - db:
            mysql: jdbc:mysql://127.0.0.1:3306/thirdeye?allowPublicKeyRetrieval=true&useSSL=false
          user: root
          password: ''
          driver: com.mysql.cj.jdbc.Driver
      PostgreSQL:
        - db:
            postgres: jdbc:postgresql://127.0.0.1:5432/thirdeye
          user: thirdeye
          password: root